<%= form_for @work do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :medium %>
  <%= f.text_field :medium %>
  <%= f.label :style %>
  <%= f.text_field :style %>
  <%= f.label :image %>
  <%= f.url_field :image, :required => true %>
  <%= f.label :musician_id %>
  <%= f.select :musician_id, Musician.pluck(:name, :id), :include_blank => true %>
  <%= f.submit %>
<% end %>
