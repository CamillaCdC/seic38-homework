<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true %>

  <%= f.label :nationality %>
  <%= f.text_field :nationality %>

  <%= f.label :members %>
  <%= f.text_field :members %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>

<% end %>
